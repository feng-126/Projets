body{
    background-color: #ffffff;
}
nav .container{
    color: #ffffff;
    line-height: 50px
}

#linkedin-tool .example{
    padding-top: 10px;
    padding-bottom: 10px;
    border-top: 2px dotted black;
    border-bottom: 2px dotted black;
    margin-top: 10px;
}
#submitForm1 .modal-body .radio_container{
    margin-bottom: 10px;
}
#submitForm1 .modal-body .radio_container label{
    font-weight: normal;
}
#submitForm1 .modal-body .radio_container input{
    margin-right: 10px;
}
#submitForm1 .modal-body #hideInput{
    display: none;
}
/*#linkedin-list moveable*/
#linkedin-list  .dd-item .dd-handle i{
    position:relative;
    cursor: move;
}
#linkedin-list .dd-item .dd-icon{
    position: absolute;
    font-size: 16px;
    left: -20px;
    top: 5px;
    color: red;
    cursor: pointer;
}
#linkedin-list .dd-list .dd-item span i{
    font-size: 16px;
    margin-top: 5px;
}

/*#linkedin-list options*/

#linkedin-options .dd-item .dd-handle{
    cursor: auto;
}
#linkedin-options .dd-list .dd-item .title{
    margin-top: 10px;
}
#linkedin-options .modal-start-btn{
	margin-top: 50px;
}
.input_required_linkedin-options{
    border: 1px solid red;
}





/* linkedin_new_experience */
#linkedin_new_experience .btn_add{
    margin-bottom: 20px;
}

#linkedin_new_experience .table_content .new_experience_icon {
    margin-top: 0px;
    font-size: 20px;
}
#linkedin_new_experience .table_content .trash i{
    cursor: pointer;
}
#linkedin_new_experience .table_content .move_bar i {
    cursor: move;
}
#linkedin_new_experience .table_content .control-label{
    font-size: 10px;
    margin-top: 10px;
}
#linkedin_new_experience .month-option{
    margin-bottom: 5px;
}

#linkedin_new_experience .today{
    display: none;
    margin-top: 35px;
}
#linkedin_new_experience .checkbox{
    margin-top: 0;
}
#linkedin_new_experience .ibox-title, #linkedin_new_experience .ibox-content{
    border: none;
    padding: 0;
    cursor: auto;
}
#linkedin_new_experience .ibox-title .up{
    position: absolute;
    top: -15px;
    right: 10px;
    font-size: 10px;
}
#linkedin_new_experience .border-bottom{
    border-bottom: none !important;
}

/* linkedin_last_etape */
#linkedin_last_etape .example{
    padding-top: 10px;
    padding-bottom: 10px;
    border-top: 2px dotted black;
    border-bottom: 2px dotted black;
    margin-top: 10px;
}
#linkedin_last_etape .content{
    margin-bottom: 30px
}
#linkedin_last_etape .content .text-area{
    width: 100%;
}
#linkedin_last_etape .content .dd-handle{
    cursor: auto;
}
#linkedin_last_etape .content .handle{
    cursor: move;
}
#linkedin_last_etape .dd-list .dd-item .dd-handle input{
    width: 90%
}
#linkedin_last_etape .content .dd-list{
    margin-bottom: 40px;
}
